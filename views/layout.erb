<!DOCTYPE html>
<html lang="en">

  <head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>OneRIM平台</title>

    <link rel="stylesheet" href="/style/app.css">

    <script src="/javascript/lib.js"></script>

    <!-- 高德地图 JavaScript -->
    <script src="http://cache.amap.com/lbs/static/es5.min.js"></script>
    <script src="http://webapi.amap.com/maps?v=1.3&key=7869a64ef800e0763b199fd87f5b1434"></script>
    <script src="//webapi.amap.com/ui/1.0/main.js"></script>

    <!-- ECharts JavaScript -->
    <script src="/echarts/echarts.common.min.js"></script>

  </head>

  <body class="sidebar-slim">

    <div class="main-wrap">
      <nav class="navigation">
        <!-- START Navbar -->
        <div class="navbar-inverse navbar navbar-fixed-top">
          <div class="container-fluid">

            <div class="navbar-header">
              <a class="current navbar-brand" href="/">
                <img alt="Spin Logo Inverted" class="h-20" src="/images/logo-primary-white2X.png">
              </a>
              <button title="" data-original-title="" class="action-right-sidebar-toggle navbar-toggle collapsed" data-target="#navdbar" data-toggle="collapse" type="button">
                <i class="fa fa-fw fa-align-right text-white"></i>
              </button>
              <button class="navbar-toggle collapsed" data-target="#navbar" data-toggle="collapse" type="button">
                <i class="fa fa-fw fa-user text-white"></i>
              </button>
              <button class="action-sidebar-open navbar-toggle collapsed" type="button">
                <i class="fa fa-fw fa-bars text-white"></i>
              </button>
            </div>

            <div class="collapse navbar-collapse" id="navbar">

              <!-- START Search Mobile -->
              <div class="form-group hidden-lg hidden-md hidden-sm">
                <div class="input-group hidden-lg hidden-md">
                  <input class="form-control" placeholder="Search for..." type="text">
                  <span class="input-group-btn">
                    <button class="btn btn-primary" type="button">
                      <i class="fa fa-fw fa-search"></i>
                    </button>
                  </span>
                </div>
              </div>
              <!-- END Search Mobile -->

              <!-- START Left Side Navbar -->
              <ul class="nav navbar-nav navbar-left clearfix yamm">

                <!-- START Switch Sidebar ON/OFF -->
                <li id="sidebar-switch" class="hidden-xs">
                  <a data-original-title="菜单的打开/隐藏" class="action-toggle-sidebar-slim" data-placement="bottom" data-toggle="tooltip" href="javascript:%20void(0)" title="">
                    <i class="fa fa-lg fa-bars fa-fw"></i>
                  </a>
                </li>
                <!-- END Switch Sidebar ON/OFF -->

                <li class="spin-search-box clearfix hidden-xs">
                  <a href="javascript:%20void(0)" class="pull-left">
                    <i class="fa fa-search fa-lg icon-inactive"></i>
                    <i class="fa fa-close fa-lg icon-active"></i>
                  </a>
                  <div class="input-group input-group-sm pull-left p-10">
                    <input class="form-control" placeholder="输入关键词查询..." type="text">
                    <span class="input-group-btn">
                      <button class="btn btn-primary" type="button" onclick="window.location.href='/search'">
                        <i class="fa fa-search"></i>
                      </button>
                    </span>
                  </div>
                </li>
              </ul>
              <!-- START Left Side Navbar -->

              <!-- START Right Side Navbar -->
              <ul class="nav navbar-nav navbar-right">



                <li class="dropdown">
                  <a class="dropdown-toggle user-dropdown" data-toggle="dropdown" href="javascript:%20void(0)" role="button">
                    <span class="m-r-1"><%= session[:current_user_name] %></span>
                    <div class="avatar avatar-image avatar-sm avatar-inline loaded">
                      <img alt="User" src="/images/avatar-default-100.png">
                    </div>
                  </a>
                  <ul class="dropdown-menu">
                    <li class="dropdown-header text-gray-lighter">
                      <strong class="text-uppercase">账号</strong>
                    </li>
                    <li>
                      <a href="/logout">退出</a>
                    </li>
                  </ul>
                </li>

              </ul>
              <!-- END Right Side Navbar -->
            </div>

          </div>
        </div>
        <!-- END Navbar -->

        <!-- START Sidebars -->
        <aside data-ps-id="61a0c5ae-300e-a9f7-0a29-d00e890030a6" class="navbar-default sidebar">

          <div class="sidebar-content">

            <div class="sidebar-default-visible text-muted small text-uppercase sidebar-section p-y-2">
              <strong>导航</strong>
            </div>

            <!-- START Tree Sidebar Common -->
            <ul class="side-menu">

              <li class="<%= 'active' if session[:current_menu].eql? 'alert' %>">
                <a href="/alert/incidents"  data-original-title="告警信息" data-placement="right" data-toggle="tooltip">
                  <i class="fa fa-warning fa-lg "></i>
                  <span class="nav-label">告警信息</span>
                </a>
              </li>

              <li class="<%= 'active' if session[:current_menu].eql? 'setting' %>">
                <a href="/setting"  data-original-title="设置" data-placement="right" data-toggle="tooltip">
                  <i class="fa fa-gear fa-lg "></i>
                  <span class="nav-label">设置</span>
                </a>
              </li>

              <li class="<%= 'active' if session[:current_menu].eql? 'testing' %>">
                <a href="/testing"  data-original-title="测试" data-placement="right" data-toggle="tooltip">
                  <i class="fa fa-user-secret fa-lg "></i>
                  <span class="nav-label">测试</span>
                </a>
              </li>

            </ul>
            <!-- END Tree Sidebar Common  -->



          </aside>
          <!-- END Sidebars -->
        </nav>

        <div style="min-height: 1316px;" class="content">
          <%= yield %>
        </div>

        <!-- START Footer -->
        <footer>
          <div class="container-fluid">
            <p class="text-gray-dark">
              <strong class="m-r-1">上海远洲管业科技股份有限公司
              </strong>
              <span class="text-gray-dark">© 2009 - 2017
                上海市, 中国</span>
            </p>
          </div>
        </footer>
        <!-- END Footer -->

      </div>

      <!-- Bower Libraries Styles -->
      <link rel="stylesheet" href="/style/lib.css">
      <script src="/javascript/app.js"></script>

      <script src="/javascript/highstock.min.js"></script>
      <script src="/javascript/highcharts-settings.js"></script>
      <script src="/javascript/highstock-settings.js"></script>
      <script src="/javascript/highcharts-highstock-theme.js"></script>
      <script src="/javascript/select2.min.js"></script>
      <script src="/javascript/bootstrap-select.min.js"></script>
      <script src="/javascript/plugins-init.js"></script>

      <script src="/javascript/common.js"></script>


    </body>

  </html>
